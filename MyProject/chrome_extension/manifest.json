{
  "manifest_version": 3,
  "name": "Search History Saver",
  "version": "1.0",
  "description": "Figyeli a felhasználó böngészési előzményeit és menti azokat a szerverre.",
  "permissions": [
    "history",
    "storage",
    "downloads",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://127.0.0.1:8000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
