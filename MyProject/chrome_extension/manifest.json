{
  "manifest_version": 3,
  "name": "Search History Saver",
  "version": "1.0",
  "description": "Mentse a böngészési előzményeket és küldje el a szerverre.",
  "permissions": [
    "history",
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://127.0.0.1:8000/*"
  ],
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
